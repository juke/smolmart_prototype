*.js    text eol=lf
*.jsx   text eol=lf
*.ts    text eol=lf
*.tsx   text eol=lf
*.json  text eol=lf
*.css   text eol=lf
*.html  text eol=lf
*.md    text eol=lf

# Ensure JavaScript files are served with the correct MIME type
*.js    linguist-language=JavaScript
*.jsx   linguist-language=JavaScript
*.ts    linguist-language=TypeScript
*.tsx   linguist-language=TypeScript 